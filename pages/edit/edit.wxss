.container {
  color: #000;
  padding: 20rpx 0 0;
  background-color: #faf9fe;
}
.header{
  width: 100%;
  padding: 10rpx 0;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 0.7em;
}
.block{
  width: 100%;
  background: #fff;
  border-bottom: 1px solid #f4f4f4;
}
.normal {
  display: flex;
  flex-direction: row;
  width: 94%;
  margin: 0 auto;
  height: 55rpx;
  align-items: center;
  padding: 15rpx 0 10rpx;
  font-size: 0.85em;
  background-color: #fff;
}

.normalInput {
  width: 500rpx;
  font-size: 1em;
  word-break: keep-all;
  white-space: nowrap;
  text-align: right;
  margin-right: 15rpx;
  overflow: hidden;
  text-overflow: ellipsis;
}

.flex {
  display: flex;
  align-items: center;
}

.flex-between {
  justify-content: space-between;
}

.flex-end {
  justify-content: flex-end;
}

.circle {
  margin-top: 20rpx;
  display: flex;
  flex-direction: row;
  width: 100%;
  height: 160rpx;
  padding: 30rpx 0;
  align-items: center;
  justify-content: center;
  background-color: #fff;
  border-bottom: 1px solid #f4f4f4;
  position: relative;
}

.circle-view {
  width: 170rpx;
  margin: 0 20rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  z-index: 10;
}

.circle>.line {
  position: absolute;
  top: 38%;
  width: 100px;
  height: 2px;
  background-color: #dedddd;
  z-index: 20;
}

.line-left {
  left: 20%;
}

.line-right {
  right: 20%;
}

.circle-text {
  margin-top: 15rpx;
  color: #666;
  font-size: 0.7em;
}

.normalmarginCircle {
  width: 110rpx;
  height: 110rpx;
  border-radius: 50%;
  background-color: #fff;
  z-index: 15;
  display: flex;
  justify-content: center;
  align-items: center;
}

.normalmarginCircle::after {
  content: "";
  width: 50rpx;
  height: 50rpx;
  border-radius: 50%;
  background-color: #dedddd;
}

.marginCircle {
  width: 110rpx;
  height: 110rpx;
  border-radius: 50%;
  background-color: #4392ee;
  z-index: 30;
  display: flex;
  justify-content: center;
  align-items: center;
}

.u249_img {
  width: 35px;
  height: 35px;
  border-radius: 50%;
}

.edit-address {
  letter-spacing: -0.08em;
}

.checkboxGroup {
  display: flex;
  justify-content: center;
  align-items: center;
  padding: 30rpx 0;
  font-family: 'Arial Normal', 'Arial';
  font-style: normal;
  font-size: 0.76em;
}

.last {
  width: 95%;
  height: 90rpx;
  margin: 0 auto;
  border-radius: 10px;
  background-color: #4392ee;
  color: #fff;
  font-size: 0.9em;
  display: flex;
  justify-content: center;
  align-items: center;
}

.author {
  width: 90rpx;
  height: 40rpx;
  background-color: #f48301;
  border-radius: 8px;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 0.77em;
  color: #fff;
}

.arrow {
  width: 15px;
  height: 15px;
}

.blue {
  color: #4392ee;
}


